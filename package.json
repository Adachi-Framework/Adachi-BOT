{
  "name": "adachi-bot",
  "private": true,
  "version": "3.2.8",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --files app.ts",
    "win-start": "ts-node app.ts",
    "stage": "cross-env NODE_ENV=production ts-node app.ts",
    "start": "pm2 start adachi-bot.config.js",
    "docker-start": "pm2-runtime start adachi-bot.config.js",
    "restart": "npm i --registry https://registry.npmmirror.com && pm2 restart adachi-bot.config.js",
    "stop": "pm2 delete adachi-bot.config.js",
    "build:client": "vite build"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.3.6",
    "echarts": "^5.4.2",
    "element-plus": "^2.3.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-ws": "^5.0.2",
    "icqq": "^0.3.14",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "md5-typescript": "^1.0.5",
    "moment": "^2.29.4",
    "node-dev": "^8.0.0",
    "node-fetch": "^2.6.1",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.1",
    "pinia": "^2.0.35",
    "pinyin": "3.0.0-alpha.5",
    "pm2": "^5.3.0",
    "puppeteer": "^19.11.1",
