    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.1",
    "@types/node-fetch": "^2.6.3",
    "@types/node-schedule": "^2.1.0",
    "@types/single-line-log": "^1.1.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "cross-env": "^7.0.3",
    "less": "^4.1.3",
    "nodemon": "^2.0.22",
    "sass": "^1.62.1",
